{
  "name": "RecentTab",
  "version": "0.0.0.1",
  "description": "Adds an 'Alt+Q' shortcut to switch back to the most recent tab",
  "permissions": ["storage", "http://*/", "https://*/"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "commands": {
    "switch-recent-tab": {
      "suggested_key": {
        "default": "Alt+Q"
      },
      "description": "Toggle recent tab"
    }
  },
  "icons": { 
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png" 
  },
  "manifest_version": 2
}
